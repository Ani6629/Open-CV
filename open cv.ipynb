    "img = cv2.imread('IMG_20190512_000714_338.jpg', 0)\n",
    "cv2.imshow('ok', img)\n",
    "k= cv2.waitKey(0) & 0xFF\n",
    "if k== 27:\n",
    "    cv2.destroyAllWindows()\n",
    "elif k== ord('i'):\n",
    "    cv2.imwrite('rudra.png', img)"
